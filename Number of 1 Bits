// c++

class Solution {
  public:
    int setBits(int N) {
    int setBits=0;
        
while(N>0)
{
    N=N&(N-1);
    setBits++;
}

    return setBits;    
    }
};

// java

class Solution {
    static int setBits(int N) {
    int setBits=0;
        
while(N>0)
{
    N=N&(N-1);
    setBits++;
}

    return setBits;    
    }
}

//python

class Solution:
	def setBits(self, N):
		setBits=0
		while(N>0):
		    N=N&(N-1)
		    setBits+=1

        return setBits
